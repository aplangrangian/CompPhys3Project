import numpy as np
#%%
A = np.random.randint(1,10+1,size = (3,3))
B = np.random.randint(1,10+1, size = (1,3))
A = np.insert(A,3,B,1)

#%%
for k in range(0, np.shape(A)[0]):
    for i in range(k+1,np.shape(A)[0]):
        for j in range(np.shape(A)[1]):
            A[i][j] = A[i][j] - A[i][k]/(A[k][k])*A[k][j]
            
