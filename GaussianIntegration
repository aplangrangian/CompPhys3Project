import numpy as np

def GaussInteg(func,N,q0):
    Q=np.zeros((N,1))
    QDERIV=np.zeros((N,1))
    for i in range(1,N+1):
        x=2/i-1
        q=q0*(1+x)/(1-x)
        qderiv=2*q0/(-1+x)**2
        Q[i-1]=q
        QDERIV[i-1]=qderiv
        print(x)
    sum=0
    return Q,QDERIV
        
print(GaussInteg(1,20,1))
